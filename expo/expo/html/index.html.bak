<script>/*<![CDATA[*/
/** ◇◆ 시계 (2018년 3월 23일 AM 10:21:50). 20180323. MoonYoungshin.
 */
(function(){

	// 호출실행
	printClock();

	// ◇◆ 시계 출력
	function printClock() {

		var d = new Date(); // 현재시각
		var nYear = d.getFullYear();
		var nMonth = d.getMonth()+1;
		var nDate =  d.getDate();
		var amPm = 'AM'; // 초기값 AM
		var nHours = addZeros(d.getHours(), 2);
		var nMinutes = addZeros(d.getMinutes() , 2);
		var nSeconds =  addZeros(d.getSeconds(), 2);
		var htmlClock = ''; // 출력 마크업
		var $clock = $('#time1weather1 .time1'); // 여기 출력

		// 시간이 12보다 클 때 PM으로 세팅, 12를 빼줌
		if(nHours >= 12){
			amPm = 'PM';
			nHours = addZeros(nHours - 12, 2);
		}

		// 50초 이상일 때 색을 변환해 준다.
		if(nSeconds >= 50){
		   nSeconds = '<em style="color:#ff0;">'+nSeconds+'</em>'
		}
		
		// 출력 마크업 생성
		htmlClock =  '<span class="dpib">' + nYear + '년 ' + nMonth + '월 ' + nDate + '일</span> ';
		//htmlClock += '<span class="dpib">' + amPm + ' ' + nHours + ':' + nMinutes + ':' + nSeconds + '</span> ';
		htmlClock += '<span class="dpib">' + amPm + ' ' + nHours + ':' + nMinutes + '</span> ';

		// 현재시각 출력
		$clock.html( htmlClock );

		// 1초마다 갱신
		// 첫 인수가 전역 함수가 아닐 때 printClock() 형태로 호출하면 오류 ))) printClock 형태로 호출해야 한다. 
		setTimeout( printClock, 1000);
	}

	// ◇◆ 자릿수 맞추기
	function addZeros(num, digit) {
		  var zero = '';
		  num = num.toString();
		  if (num.length < digit) {
			for (i = 0; i < digit - num.length; i++) {
			  zero += '0';
			}
		  }
		  return zero + num;
	}

	// 날씨관련정보
	if(0 == 0){
		if(3 == 4){
            $('#time1weather1 .weather1 .t1').html("흐림");
            $('#time1weather1 .weather1 .ic1').html("<img src='/_res/portal/img/main/weather1/w101.png' alt='흐림' />");
        }else if(3 == 3){
            $('#time1weather1 .weather1 .t1').html("구름많음");
            $('#time1weather1 .weather1 .ic1').html("<img src='/_res/portal/img/main/weather1/w101.png' alt='구름많음' />");
        }else if(3 == 2){
            $('#time1weather1 .weather1 .t1').html("구름조금");
            $('#time1weather1 .weather1 .ic1').html("<img src='/_res/portal/img/main/weather1/w101.png' alt='구름조금' />");
        }else{
            $('#time1weather1 .weather1 .t1').html("맑음");
            $('#time1weather1 .weather1 .ic1').html("<img src='/_res/portal/img/main/weather1/w101.png' alt='맑음' />");
        }
	}else if(0 == 1){
			$('#time1weather1 .weather1 .t1').html("비");
            $('#time1weather1 .weather1 .ic1').html("<img src='/_res/portal/img/main/weather1/w101.png' alt='비' />");
	}else if(0 == 2){
			$('#time1weather1 .weather1 .t1').html("비/눈");
            $('#time1weather1 .weather1 .ic1').html("<img src='/_res/portal/img/main/weather1/w101.png' alt='비/눈' />");
	}else{
			$('#time1weather1 .weather1 .t1').html("눈");
            $('#time1weather1 .weather1 .ic1').html("<img src='/_res/portal/img/main/weather1/w101.png' alt='눈' />");
	}

	

})();
/*]]>*/</script>

